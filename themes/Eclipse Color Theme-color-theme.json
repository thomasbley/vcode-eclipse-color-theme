{
	"name": "Eclipse Color Theme",
	"type": "light",
	"colors": {
		"editor.background": "#fcfcfc",
		"editor.foreground": "#000000",
		"editorLineNumber.foreground": "#787878",
		"editorGutter.background": "#F3F3F3",
		"editorLineNumber.activeForeground": "#787878",
		"editor.lineHighlightBackground": "#e8f2fe",
		"editor.findMatchBackground": "#ceccf7",
		"editor.selectionBackground": "#d4e8fd",
		"editor.selectionHighlightBackground": "#f0d8a8",
		"editor.wordHighlightBackground": "#d4d4d4",
		"editor.wordHighlightStrongBackground": "#f0d8a8",
		"editor.inactiveSelectionBackground": "#d4e8fd",
		"sideBar.border": "#ECECEC"
	},
	"tokenColors": [
		{
			"name": "php-doc-type",
			"scope": [
				"comment.block.documentation.phpdoc.php support.class.php",
				"comment.block.documentation.phpdoc.php keyword.other.type.php",
				"comment.block.documentation.phpdoc.php keyword.other.array.phpdoc.php",
				"comment.block.documentation.phpdoc.php support.other.namespace.php"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#3F55BF"
			}
		},
		{
			"name": "php-variable-global",
			"scope": [
				"variable.other.global.php",
				"variable.other.global.safer.php"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#800555"
			}
		},
		{
			"name": "php-comment-line",
			"scope": [
				"comment.line.double-slash.php",
				"comment.block.php"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#557F5F"
			}
		},
		{
			"name": "php-static-call",
			"scope": [
				"meta.method-call.static.php entity.name.function"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#000000"
			}
		},
		{
			"name": "php-protected",
			"scope": [
				"storage.modifier.php"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#22804C"
			}
		},
		{
			"name": "php-constant",
			"scope": [
				"constant.other.php"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#0000C0"
			}
		},
		{
			"name": "php-class-type-hint",
			"scope": [
				"support.class.php",
				"support.other.namespace.php"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "php-property",
			"scope": [
				"variable.other.property.php"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#0000C0"
			}
		},
		{
			"name": "php-empty",
			"scope": [
				"support.function.construct.php"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#800555"
			}
		},
		{
			"name": "php-begin",
			"scope": [
				"punctuation.section.embedded.begin.php"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#FF0000"
			}
		},
		{
			"name": "php-comments",
			"scope": [
				"keyword.other.phpdoc.php"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7F9FBF"
			}
		},
		{
			"name": "Comments",
			"scope": [
				"comment"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#3F55BF"
			}
		},
		{
			"name": "Strings",
			"scope": [
				"string"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#0000C0"
			}
		},
		{
			"name": "String: Escape sequences and placeholders",
			"scope": [
				"constant.character.escape",
				"constant.other.placeholder"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#7090FF"
			}
		},
		{
			"name": "Numbers",
			"scope": [
				"constant.numeric",
				"keyword.other.unit"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Keywords and built-in types",
			"scope": [
				"keyword",
				"storage.type.primitive",
				"storage.type.class",
				"storage.type.struct",
				"storage.type.enum",
				"storage.type.modifier",
				"storage.type.namespace",
				"storage.type.template",
				"storage.type.function",
				"storage.type.union",
				"storage.type.js",
				"storage.type.interface",
				"storage.type.impl",
				"storage.type.trait",
				"storage.modifier",
				"keyword.operator.new",
				"keyword.operator.delete",
				"keyword.operator.word",
				"keyword.operator.sizeof",
				"keyword.operator.alignof",
				"keyword.operator.logical.python",
				"constant.language",
				"keyword.declaration.function",
				"keyword.declaration.class",
				"keyword.declaration.struct",
				"keyword.declaration.enum",
				"keyword.declaration.union",
				"keyword.declaration.trait",
				"keyword.declaration.interface",
				"keyword.declaration.impl",
				"keyword.declaration.type"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#800555"
			}
		},
		{
			"name": "Operators",
			"scope": [
				"keyword.operator"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Types",
			"scope": [
				"entity.name.type",
				"storage.type"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Template Types",
			"scope": [
				"entity.name.type.template"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#000000"
			}
		},
		{
			"name": "CUDA Kernel Brackets",
			"scope": [
				"meta.kernel-call punctuation.section.kernel"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#000000"
			}
		},
		{
			"name": "CUDA Kernel Call",
			"scope": [
				"meta.kernel-call entity.name.function"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Object Members and Attributes",
			"scope": [
				"variable.other.member",
				"variable.other.property"
			],
			"settings": {
				"foreground": "#333333",
				"fontStyle": ""
			}
		},
		{
			"name": "Functions",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#000000"
			}
		},
		{
			"name": "Preprocessor: Other",
			"scope": [
				"meta.preprocessor"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#808080"
			}
		},
		{
			"name": "Preprocessor: Directives",
			"scope": [
				"meta.preprocessor keyword.control.directive"
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#7F3F6A"
			}
		},
		{
			"name": "Preprocessor: Headers",
			"scope": [
				"meta.preprocessor string.quoted"
			],
			"settings": {
				"fontStyle": "",
				"foreground": "#9580FF"
			}
		},
		{
			"name": "SDK types and functions",
			"scope": [
				"support.function",
				"support.type.python",
				"support.type.posix-reserved"
			],
			"settings": {
				"foreground": "#000000",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Python: self",
			"scope": [
				"variable.parameter.function.language.special.self",
				"variable.language.special.self",
				"variable.parameter.function.language.special.cls",
				"variable.language.special.cls"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#000000"
			}
		},
		{
			"name": "Decorators",
			"scope": [
				"entity.name.function.decorator"
			],
			"settings": {
				"foreground": "#7D7D7D",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Docstrings",
			"scope": [
				"string.quoted.docstring"
			],
			"settings": {
				"foreground": "#406080",
				"fontStyle": ""
			}
		},
		{
			"name": "HTML/XML Tags",
			"scope": [
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#800555",
				"fontStyle": "bold"
			}
		},
		{
			"name": "HTML/XML Attributes",
			"scope": [
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#800555",
				"fontStyle": ""
			}
		},
		{
			"name": "HTML/XML Meta information",
			"scope": [
				"meta.tag.preprocessor entity.name.tag",
				"meta.tag.preprocessor entity.other.attribute-name",
				"meta.tag.preprocessor string",
				"meta.tag.preprocessor text",
				"meta.tag.metadata.doctype entity.name.tag",
				"meta.tag.metadata.doctype entity.other.attribute-name",
				"meta.tag.metadata.doctype string",
				"meta.tag.metadata.doctype text"
			],
			"settings": {
				"foreground": "#5F8FBF",
				"fontStyle": "italic"
			}
		},
		{
			"name": "Markup: Section Heading",
			"scope": [
				"entity.name.section",
				"punctuation.definition.heading"
			],
			"settings": {
				"foreground": "#800555",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Raw Inline",
			"scope": [
				"markup.inline.raw",
				"punctuation.definition.raw"
			],
			"settings": {
				"foreground": "#FF00AA",
				"fontStyle": ""
			}
		},
		{
			"name": "Markup: Lists",
			"scope": [
				"punctuation.definition.list"
			],
			"settings": {
				"foreground": "#000000",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Bold",
			"scope": [
				"markup.bold"
			],
			"settings": {
				"foreground": "#000000",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Markup: Underline",
			"scope": [
				"markup.underline"
			],
			"settings": {
				"foreground": "#000000",
				"fontStyle": "underline"
			}
		},
		{
			"name": "Markup: Italic",
			"scope": [
				"markup.italic"
			],
			"settings": {
				"foreground": "#000000",
				"fontStyle": "italic"
			}
		},
		{
			"name": "TODOs",
			"scope": [
				"keyword.codetag"
			],
			"settings": {
				"foreground": "#86A3BF",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Support",
			"scope": [
				"support"
			],
			"settings": {
				"foreground": "#000000",
				"fontStyle": ""
			}
		}
	]
}
